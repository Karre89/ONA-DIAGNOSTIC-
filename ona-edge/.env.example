# ONA Edge Configuration
# Copy this file to .env and fill in the values

# Device identification (unique per device)
ONA_DEVICE_ID=ONA-KE-001
ONA_SITE_ID=site-kenya-nairobi-01

# ONA Cloud API
ONA_CLOUD_API=https://ona-diagnostic-production.up.railway.app/api/v1
ONA_API_KEY=your-device-token-here

# DICOM settings
ONA_DICOM_PORT=11112
ONA_AE_TITLE=ONA_EDGE

# AI settings (cpu or cuda)
ONA_DEVICE=cpu

# UI settings
ONA_UI_PORT=8080

# Sync settings
ONA_SYNC_INTERVAL=60
