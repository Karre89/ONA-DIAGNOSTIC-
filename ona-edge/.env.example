# ONA Edge Configuration
# Copy this file to .env and fill in the values
#
# To get your device credentials, run:
#   python tools/register_device.py

# Device identification (from cloud registration)
ONA_DEVICE_ID=your-device-uuid-here
ONA_SITE_ID=your-site-uuid-here
ONA_TENANT_ID=your-tenant-uuid-here

# ONA Cloud API
ONA_CLOUD_API=https://ona-diagnostic-production.up.railway.app/api/v1
ONA_API_KEY=your-device-token-here

# DICOM settings
ONA_DICOM_PORT=11112
ONA_AE_TITLE=ONA_EDGE

# AI settings (cpu or cuda)
ONA_DEVICE=cpu

# UI settings
ONA_UI_PORT=8080

# Sync settings
ONA_SYNC_INTERVAL=60
